{
  "name": "circle-test",
  "version": "1.0.0",
  "description": "Test project for CircleCi integration",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "build": "webpack --mode production --env.api-prefix=/circle/",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "lint": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stats": "webpack --mode production --json --optimize-minimize > stats.json",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:seed:all": "sequelize db:seed:all",
    "db:nuke": "npx npm-run-all -s db:drop db:create",
    "db:recreate": "npx npm-run-all -s db:nuke db:migrate db:seed:all",
    "test:build": "webpack --mode production",
    "test:start": "cross-env NODE_ENV=test node index.js",
    "test:setup": "npx npm-run-all -s build db:migrate db:seed:all"
  },
  "engines": {
    "node": ">=10.15"
  },
  "author": "Jami Kousa",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.3",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^1.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "module-alias": "^2.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "react": "^v16.8.1",
    "react-dom": "^v16.8.1",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.85.0",
    "sequelize": "^5.8.10",
    "sequelize-cli": "^5.5.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "wait-on": "^3.2.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-preset-stage-2": "^6.24.1",
    "cypress": "^3.3.1",
    "nodemon": "^1.18.10",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.23.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "client/*"
    ]
  },
  "_moduleAliases": {
    "@root": ".",
    "@controllers": "server/controllers",
    "@util": "server/util"
  }
}
